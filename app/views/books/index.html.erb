<h1>Books</h1>
<% @books.each do |book| %>
<p>Title</p>
<span><%= book.title %></span>
<% end %>

<h1>New book</h1>
<!-- form_with部分 -->
<%= form_with model: @book, url:'/books', local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
<% end %>

